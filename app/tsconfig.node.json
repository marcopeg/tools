{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "target": "es2023", // or "esnext"
    "lib": ["dom", "es2023"] // or ["dom", "esnext"]
  },
  "include": ["vite.config.ts"]
}
